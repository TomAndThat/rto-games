#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/Crucial X9/Dev/rto-games/node_modules/.pnpm/vitest@4.0.18_@opentelemetry+api@1.9.0_@types+node@20.19.33_@vitest+ui@4.0.18_happy-dom_7d8d98e8a9a3c4dcb49df91a1728a052/node_modules/vitest/node_modules:/Volumes/Crucial X9/Dev/rto-games/node_modules/.pnpm/vitest@4.0.18_@opentelemetry+api@1.9.0_@types+node@20.19.33_@vitest+ui@4.0.18_happy-dom_7d8d98e8a9a3c4dcb49df91a1728a052/node_modules:/Volumes/Crucial X9/Dev/rto-games/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/Crucial X9/Dev/rto-games/node_modules/.pnpm/vitest@4.0.18_@opentelemetry+api@1.9.0_@types+node@20.19.33_@vitest+ui@4.0.18_happy-dom_7d8d98e8a9a3c4dcb49df91a1728a052/node_modules/vitest/node_modules:/Volumes/Crucial X9/Dev/rto-games/node_modules/.pnpm/vitest@4.0.18_@opentelemetry+api@1.9.0_@types+node@20.19.33_@vitest+ui@4.0.18_happy-dom_7d8d98e8a9a3c4dcb49df91a1728a052/node_modules:/Volumes/Crucial X9/Dev/rto-games/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
